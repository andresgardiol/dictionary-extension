{
  "manifest_version": 3,
  "name": "Personal Dictionary",
  "description": "A personal dictionary for your work terms",
  "version": "1.4",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "dictionary.png"
  },
  "permissions": [
    "storage",
    "clipboardWrite",
    "clipboardRead"
  ],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Q",
        "mac": "MacCtrl+Q"
      }
    }
  }
}
